{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2017"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "target",
    "preserveWatchOutput": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2017"
  }
}